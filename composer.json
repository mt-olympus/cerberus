{
    "name": "los/cerberus",
    "type": "library",
    "homepage": "https://github.com/Lansoweb/cerberus",
    "license": "MIT",
    "authors": [
        {
            "name": "Leandro Silva",
            "homepage": "http://leandrosilva.info/"
        }
    ],
    "require": {
        "roave/security-advisories": "dev-master",
        "zendframework/zend-expressive": "1.0.0rc2",
        "zendframework/zend-stdlib": "~2.7",
        "zendframework/zend-expressive-fastroute": "^0.2",
        "zendframework/zend-servicemanager": "^2.5",
        "ocramius/proxy-manager": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Cerberus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CerberusTest\\": "test/"
        }
    },
    "scripts": {
        "test": [
            "php vendor/squizlabs/php_codesniffer/scripts/phpcs",
            "php vendor/phpunit/phpunit/phpunit"
        ]
    }
}
