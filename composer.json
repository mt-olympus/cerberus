{
  "name" : "mt-olympus/cerberus",
  "description": "Circuit Breaker pattern implementation in PHP",
  "require" : {
    "php" : "^5.5|^7.0",
    "zendframework/zend-stdlib" : "^2.5",
    "zendframework/zend-servicemanager" : "^2.5",
    "zendframework/zend-cache" : "^2.4"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^5.0",
    "squizlabs/php_codesniffer" : "^2.3",
    "satooshi/php-coveralls" : "~0.6"
  },
  "license" : "MIT",
  "autoload-dev" : {
    "psr-4" : {
      "CerberusTest\\" : "test/"
    }
  },
  "autoload" : {
    "psr-4" : {
      "Cerberus\\" : "src/"
    }
  },
  "type" : "library",
  "scripts" : {
    "test" : [ "php vendor/squizlabs/php_codesniffer/scripts/phpcs", "php vendor/phpunit/phpunit/phpunit" ]
  },
  "homepage" : "https://github.com/mt-olympus/cerberus",
  "authors" : [ {
    "name" : "Leandro Silva",
    "homepage" : "http://leandrosilva.info/"
  } ]
}
